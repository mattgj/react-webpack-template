{
    "optional": [
        "es7.decorators",
        "es7.objectRestSpread"
    ],
    "env": {
        "start": {
            "plugins": [
                "react-transform"
            ],
            "extra": {
                "react-transform": {
                    "transforms": [{
                        "transform": "react-transform-hmr",
                        "imports": ["react"],
                        "locals": ["module"]
                    }, {
                        "transform": "react-transform-catch-errors",
                        "imports": [
                            "react",
                            "redbox-react"
                        ]
                    }]
                }
            }
        }
    }
}
